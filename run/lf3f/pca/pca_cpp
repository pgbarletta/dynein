parm ../../../top_files/dry_lf3f.prmtop
trajin ../pdt/full_fit_lf3f.nc
rms first :1-204@CA
average avg_lf3f.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference avg_lf3f.pdb [avg]
rms ref [avg] :1-204@CA RMSD
matrix mwcovar name mtx_lf3f :1-204@CA
createcrd crd_lf3f
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_lf3f vecs 606 out modes_lf3f
go
