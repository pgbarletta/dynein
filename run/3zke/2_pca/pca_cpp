parm ../../../top_files/dry_3zke.prmtop
trajin ../2_pdt/2_full_fit_3zke.nc
rms first :1-188@CA
average 2_avg_3zke.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference 2_avg_3zke.pdb [avg]
rms ref [avg] :1-188@CA RMSD
matrix mwcovar name mtx_3zke :1-188@CA
createcrd crd_3zke
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_3zke vecs 558 out 2_modes_3zke
go
