parm ../../../top_files/dry_3fm7.prmtop
trajin ../4_pdt/4_full_fit_3fm7.nc
rms first :1-429@CA
average 4_avg_3fm7.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference 4_avg_3fm7.pdb [avg]
rms ref [avg] :1-429@CA RMSD
matrix mwcovar name mtx_3fm7 :1-429@CA
createcrd crd_3fm7
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_3fm7 vecs 1281 out 4_modes_3fm7
go
