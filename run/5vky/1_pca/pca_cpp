parm ../../../top_files/dry_5vky.prmtop
trajin ../1_pdt/1_full_fit_5vky.nc
rms first :1-224@CA
average 1_avg_5vky.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference 1_avg_5vky.pdb [avg]
rms ref [avg] :1-224@CA RMSD
matrix mwcovar name mtx_5vky :1-224@CA
createcrd crd_5vky
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_5vky vecs 666 out 1_modes_5vky
go
