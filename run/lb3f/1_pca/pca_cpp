parm ../../../top_files/dry_lb3f.prmtop
trajin ../1_pdt/1_full_fit_lb3f.nc
rms first :1-232@CA
average 1_avg_lb3f.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference 1_avg_lb3f.pdb [avg]
rms ref [avg] :1-232@CA RMSD
matrix mwcovar name mtx_lb3f :1-232@CA
createcrd crd_lb3f
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_lb3f vecs 693 out 1_modes_lb3f
go
