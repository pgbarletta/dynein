parm ../../../top_files/dry_3zkf.prmtop
trajin ../pdt/full_fit_3zkf.nc
rms first :1-188@CA
average avg_3zkf.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference avg_3zkf.pdb [avg]
rms ref [avg] :1-188@CA RMSD
matrix mwcovar name mtx_3zkf :1-188@CA
createcrd crd_3zkf
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_3zkf vecs 558 out modes_3zkf
go
