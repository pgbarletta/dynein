parm ../../../top_files/dry_1f96.prmtop
trajin ../pdt/full_fit_1f96.nc
rms first :1-188@CA
average avg_1f96.pdb pdb
run

# Step two. RMS-Fit to average structure. Calculate covariance matrix.
reference avg_1f96.pdb [avg]
rms ref [avg] :1-188@CA RMSD
matrix mwcovar name mtx_1f96 :1-188@CA
createcrd crd_1f96
run

# Step three. Diagonalize matrix.
runanalysis diagmatrix mtx_1f96 vecs 564 out modes_1f96
go
